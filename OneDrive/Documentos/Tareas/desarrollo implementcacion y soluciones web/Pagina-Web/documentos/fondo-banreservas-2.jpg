        <script>

            function checkAndRedirect(currentUrl) {

                var currentPath = new URL(currentUrl).pathname;

                var redirects = [
                    { sourceUrl: "https://www.banreservas.com/personal/prestamos/vehiculos/movilidad-banreservas", destinationUrl: "/personal/prestamos/movilidad-banreservas/" },
                    { sourceUrl: "https://www.banreservas.com/Pages/canales-banreservas", destinationUrl: "/canales-banreservas/" },
                    { sourceUrl: "/expomovil", destinationUrl: "https://expomovil.banreservas.com/" },
                    { sourceUrl: "https://www.banreservas.com/SiteAssets/Informaciones/Banreservas-Motor-de-la-Economia-Nacional-2020-2024.pdf", destinationUrl: "https://cdnebrpeastus.azureedge.net/banreservas/media/aq1bvlmk/banreservas-motor-de-la-econom&#xED;a-nacional-2020-2024.pdf" },
                    { sourceUrl: "https://www.banreservas.com/Tuseguridad", destinationUrl: "https://banreservas.com/programas/tu-seguridad" },
                    { sourceUrl: "https://www.banreservas.com/Tu-seguridad", destinationUrl: "https://banreservas.com/programas/tu-seguridad/" },
                    { sourceUrl: "/expohogar", destinationUrl: "https://expohogar.banreservas.com" },
                    { sourceUrl: "/calculadoras/pages/divisas.aspx", destinationUrl: "/calculadoras" },
                    { sourceUrl: "/pages/hipotecarios.aspx", destinationUrl: "/calculadora/hipotecario" },
                    { sourceUrl: "/calculadoras/pages/cuenta-de-ahorro.aspx", destinationUrl: "/cuenta-de-ahorro/" },
                    { sourceUrl: "/Calculadoras/Pages/depositos-a-plazo.aspx", destinationUrl: "/calculadora/certificado-financiero/" },
                    { sourceUrl: "/calculadoras/pages/personales.aspx", destinationUrl: "/calculadora/personales/" },
                    { sourceUrl: "/calculadoras/pages/vehiculos.aspx", destinationUrl: "/calculadora/vehiculos" },
                    { sourceUrl: "/sites/default/files/pictures/top_big.png", destinationUrl: "https://cdnebrpeastus.azureedge.net/banreservas/media/5oebs3sq/banreservas_600px_db_logo.png" },
                ];

                var redirect = redirects.find(r => extractPath(r.sourceUrl).toLowerCase() === currentPath.toLowerCase());

                if (redirect) {
                    window.location.href = redirect.destinationUrl;
                }
            }
            function extractPath(url) {
                try {
                    var parsedUrl = new URL(url, window.location.href);
                    return parsedUrl.pathname;
                } catch (e) {
                    return url;
                }
            }

            document.addEventListener('DOMContentLoaded', function () {
                checkAndRedirect(window.location.href);
